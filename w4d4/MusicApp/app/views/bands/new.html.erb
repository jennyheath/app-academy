<%= render "shared/errors" %>

<h2>Create a band</h2>

<form action="<%= bands_url %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <label>Name
    <input type="text" name="band[name]" value="<%= @band.name %>">
  </label>
  <br><br>

  <input type="Submit" type="Create Band">
</form>
